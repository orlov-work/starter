{
  "name": "evico2",
  "version": "0.0.0",
  "scripts": {
    "start": "nodemon src/server.js --exec babel-node",
    "build": "npm run lint && babel src -d dist",
    "serve": "node dist/server.js",
    "db:migrate": "knex migrate:latest --knexfile ./knex/knexfile.js",
    "db:rollback": "knex migrate:rollback --knexfile ./knex/knexfile.js",
    "db:seed": "knex seed:run --knexfile ./knex/knexfile.js",
    "lint": "eslint src"
  },
  "dependencies": {
    "babyparse": "^0.4.6",
    "body-parser": "^1.18.2",
    "config": "^1.30.0",
    "elasticsearch": "^14.2.2",
    "express": "^4.16.2",
    "knex": "^0.14.6",
    "libaio": "^1.0.3",
    "lodash": "^4.17.10",
    "oracle": "^0.4.1",
    "oracledb": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-import": "^2.7.0",
    "nodemon": "^1.14.12"
  }
}
