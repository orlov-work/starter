  "ava": {
    "babel": "inherit",
    "sources": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "files": [
      "./_dist/spec.bundle.js"
    ],
    "concurrency": 5,
    "failWithoutAssertions": false,
    "verbose": true,
    "require": [
      "babel-register",
      "./test/setup-test-env.js"
    ]
  },
  "nyc": {
    "reporter": "lcov",
    "exclude": [
      "_dist",
      "dist",
      "**/*.spec.js"
    ],
    "report-dir": "reports/coverage",
    "lines": 91,
    "check-coverage": true
  }
