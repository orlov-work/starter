    "clean": "rimraf ./dist",
    "precompile-tests": "cross-env NODE_ENV=test webpack",
    "clean-test": "rimraf ./_dist",
    "test": "npm run precompile-tests && ava --verbose",
    "test:cover": "npm run precompile-tests && nyc ava"
